@model EnclosureServiceModel

<div class="container text-center">
    <div class="row align-items-start">
        <div class="col">
            Parts:<br />
            @if (Model.Parts?.Count() == 0)
            {
                <text>No parts for this enclosure yet.</text>
            }
            else
            {
                @foreach (var part in Model.Parts)
                {
                    <text>@part.DeviceTag - @part.Manufacturer - @part.OrderNumber - @part.Description</text>
                    <br />
                }
            }
        </div>
    </div>
</div>
