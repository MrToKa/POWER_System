@model List<PartViewModel>

<div class="container text-center">
    <div class="row align-items-start">
        <div class="col">
            Parts:<br />
            @if (Model.Count() == 0)
            {
                <text>No parts for this enclosure yet.</text>
            }
            else
            {
                @foreach (var part in Model)
                {
                    <text>@part.Manufacturer - @part.OrderNumber - @part.Description - @part.Quantity</text>
                    <br />
                }
            }
        </div>
    </div>
</div>